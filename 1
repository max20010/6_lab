#include "stdio.h"
int main()
{
	int x, min,x_min,save,fr=0;
	scanf("%d",&x);
	int m[x];
	for (int i = 0;i < x;i++) {
		scanf("%d",&m[i]);
	}
	
	for (fr;fr <= x-2;fr++) {
		min = m[fr];
		for (int i = fr;i < x;i++) {
			if (min > m[i]) {
				min = m[i];
				x_min = i;
				save = m[fr];
				m[fr] = min;
				m[x_min] = save;
			}
		}
	}

	for (int i = 0;i < x;i++) {
		printf(" %d",m[i]);
	}
	printf("\n");
	return 0;
}
