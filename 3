#include "stdio.h"
int main()
{
	int n, s[1][1], ch = 1;
	scanf("%d",&n);
	int m[n][n];

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			scanf("%d",&m[i][j]);
		}
	}
	for (;ch != 0;) {
		ch = 0;
		for (int i = 0;i < n - 1;i++) {
			if (m[i][i] > m[i + 1][i + 1]) {
				s[0][0] = m[i][i];
				m[i][i] = m[i + 1][i + 1];
				m[i + 1][i + 1] = s[0][0];
				ch++;
			}
		}
	}

	for (int i = 0;i < n;i++) {
		for (int j = 0;j < n;j++) {
			printf("%d\t",m[i][j]);
		}
		printf("\n");
	}
	return 0;
}
