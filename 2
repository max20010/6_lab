#include "stdio.h"
int main()
{
	int x, n = 0, ch = 1, sw, s;
	scanf("%d",&x);
	int m[x];
	for (int i = 0;i < x;i++) {
		scanf("%d",&m[i]);
	}

	for (;ch!=0;) {
		ch = 0;
		for (int i = 0;i < x - 1;i++) {
			if (m[i] < m[i + 1]) {
				sw = m[i];
				m[i] = m[i + 1];
				m[i + 1] = sw;
				ch++;
			}
		}
	}

	for (int i = 0;i < x;i++) {
		printf("%d",m[i]);
	}
	printf("\n");
	return 0;
}
